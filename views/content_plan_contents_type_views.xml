<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="content_plan_contents_type_action" model="ir.actions.act_window">
        <field name="name">Content Types</field>
        <field name="res_model">content.plan.contents.type</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="content_plan_contents_type_view_form" model="ir.ui.view">
        <field name="name">content.plan.contents.type.view.form</field>
        <field name="model">content.plan.contents.type</field>
        <field name="arch" type="xml">
            <form string="Content Type">
                <sheet>
                    <field name="name" class="h2"></field>
                </sheet>
            </form>
        </field>
    </record> 

    <record id="content_plan_contents_type_view_tree" model="ir.ui.view">
        <field name="name">content.plan.contents.type.view.tree</field>
        <field name="model">content.plan.contents.type</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" string="type name"></field>
            </tree>
        </field>
    </record>

</odoo>